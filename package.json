  "license": "MIT",
  "private": true,
  "dependencies": {
    "ipfs-css": "^1.3.0",
    "multiformats": "^9.7.1",
    "runes2": "^0.4.16",
    "tachyons": "^4.12.0",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "node-fetch": "^3.2.9",
    "parcel": "^2.6.2",
    "patch-package": "^6.4.7"
  }
}
