{
  "name": "cid-utils-website",
  "version": "1.1.0",
  "description": "Website for decoding CIDs",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist/*",
    "start": "parcel index.html",
    "build": "parcel build index.html --public-url '.'",
    "postinstall": "patch-package",
    "deploy": "ipfs add -r --cid-version 1 --quieter dist"
  },
  "keywords": [],
